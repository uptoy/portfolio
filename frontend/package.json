{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "sample": "next build && next start",
    "lint": "next lint",
    "lint:fix": "eslint src --config .eslintrc.json --ext .ts,.tsx --fix",
    "lint:fixCypress": "eslint cypress --config cypress/.eslintrc.json --ext .ts,.tsx --fix",
    "prettier": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "jest": "jest",
    "ci:cy": "start-server-and-test 'yarn build && yarn start' http://localhost:3000 'cypress run --spec 'cypress/integration/test/**''",
    "test": "yarn jest && yarn ci:cy",
    "format": "yarn run prettier && yarn run lint:fix && yarn run lint:fixCypress",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "11.9.3",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.9.3",
    "@hookform/resolvers": "^2.9.3",
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.8.6",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "markdown-to-jsx": "^7.1.7",
    "next": "12.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-hot-toast": "^2.2.0",
    "react-images-uploading": "^3.1.6",
    "recharts": "^2.1.12",
    "recoil": "^0.7.4",
    "swiper": "^8.2.6",
    "swr": "^1.3.0",
    "yup": "^0.32.11",
    "lodash": "^4.17.21",
    "react-dropzone": "^12.0.5",
    "react-number-format": "^4.9.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/node": "18.0.0",
    "@types/react": "17.0.3",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "typescript": "4.7.4",
    "@babel/core": "^7.18.2",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/react": "^6.5.8",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.179",
    "@types/react-dom": "18.0.5",
    "babel-loader": "^8.2.5",
    "cypress": "^9.6.1",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "start-server-and-test": "^1.14.0"
  }
}
