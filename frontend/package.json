    "postcss": "^8",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-dropzone": "^12.0.5",
    "react-hook-form": "^7.24.0",
    "react-hooks-form": "^2.0.1",
    "react-hot-toast": "^2.2.0",
    "react-images-uploading": "^3.1.3",
    "react-number-format": "^4.9.1",
    "react-toastify": "^8.2.0",
    "recharts": "^2.1.9",
    "recoil": "^0.7.2",
